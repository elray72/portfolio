{
  "name": "react-portfolio",
  "version": "1.0.0",
  "description": "Boilerplate for creating a minimalistic web developer portfolio. Built with React and packaged by Webpack4.",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "jest --watchAll --coverage",
    "precommit": "lint-staged",
    "fix": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elray72/react-portfolio.git"
  },
  "keywords": [
    "react",
    "webpack",
    "sass",
    "idangerous",
    "swiper",
    "mixitup",
    "jest"
  ],
  "author": "Ray Ranola",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elray72/react-portfolio/issues"
  },
  "homepage": "https://github.com/elray72/react-portfolio#readme",
  "lint-staged": {
    "*.{js,jsx,css}": [
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "lodash": "^4.17.11",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "4.3.4",
    "react-id-swiper": "^1.6.8",
    "react-redux": "^5.0.7",
    "react-responsive-image": "^5.0.0",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^4.0.0",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.16.0",
    "sanitize.css": "^7.0.3",
    "shufflejs": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.56",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.56",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.56",
    "@babel/plugin-proposal-function-sent": "7.0.0-beta.56",
    "@babel/plugin-proposal-json-strings": "7.0.0-beta.56",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-beta.56",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.56",
    "@babel/plugin-syntax-import-meta": "7.0.0-beta.56",
    "@babel/plugin-transform-runtime": "7.0.0-beta.56",
    "@babel/polyfill": "7.0.0-beta.56",
    "@babel/preset-env": "7.0.0-beta.56",
    "@babel/preset-react": "7.0.0-beta.56",
    "@babel/register": "7.0.0-beta.56",
    "@babel/runtime": "7.0.0-beta.56",
    "autoprefixer": "^9.1.5",
    "babel-core": "7.0.0-beta.3",
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-lodash": "3.3.4",
    "bows": "^1.7.0",
    "browserslist": "4.0.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jest": "21.18.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "eslint-watch": "4.0.2",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.4.2",
    "jest-enzyme": "^6.0.2",
    "lint-staged": "7.2.0",
    "mini-css-extract-plugin": "0.4.1",
    "mixitup": "^3.3.1",
    "node-sass": "4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "pretty-quick": "1.6.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.22.0",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "4.1.4"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  }
}
